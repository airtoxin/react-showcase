{
  "name": "react-showcase",
  "version": "0.1.0",
  "description": "React.js component that helps compoent showcases",
  "main": "lib/index.js",
  "scripts": {
    "build": "rimraf lib && babel src -d lib --copy-files",
    "watch": "rimraf lib && babel src -d lib --copy-files -w",
    "lint": "eslint {src,test} --fix",
    "test": "npm run lint && mocha --compilers js:babel-register test",
    "prepublish": "npm run build"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/airtoxin/react-showcase.git"
  },
  "keywords": [
    "react",
    "react-component",
    "showcase",
    "atomic design"
  ],
  "author": "airtoxin <airtoxin@icloud.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/airtoxin/react-showcase/issues"
  },
  "homepage": "https://github.com/airtoxin/react-showcase#readme",
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-react": "^6.24.1",
    "babel-register": "^6.26.0",
    "enzyme": "2.4.1",
    "eslint": "^4.11.0",
    "eslint-config-airbnb": "^16.1.0",
    "eslint-plugin-import": "^2.8.0",
    "eslint-plugin-jsx-a11y": "^6.0.2",
    "eslint-plugin-react": "^7.5.1",
    "mocha": "^4.0.1",
    "react": "15.3.0",
    "react-addons-test-utils": "15.3.0",
    "react-dom": "15.3.0"
  }
}
